def main(): #main function

    fNum=int(input('Enter the first integer: '))
    sNum=int(input('Enter the second integer: '))

    print('The number ' + str(max_of_two(fNum, sNum)) + ' is greater.')

def max_of_two(fNum, sNum):
    if (fNum>sNum):
        return fNum
    else:
        return sNum

